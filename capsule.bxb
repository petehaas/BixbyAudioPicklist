capsule {
  id (playground.audioPickList)
  version (0.1.0)
  format (3)
  targets {
    target (bixby-mobile-en-US)
  }
  runtime-flags {
    concepts-inherit-super-type-features
    modern-prompt-rejection
    support-halt-effect-in-computed-inputs
  }
  
   capsule-imports {
    import (bixby.audioPlayer) {
      version (0.3.6)
      as (audioPlayer)
    }
  }
}
